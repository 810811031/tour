page {
	background-color: rgb(240, 242, 245);
}

.wrapper {
	width: 100%;
}

.wrapper .commond {
	width: 100%;
	height: 100rpx;
	background-color: #fff;
	box-sizing: border-box;
	padding: 0 33rpx;
	display: flex;
	justify-content: space-between;
	align-items: center;
}

.wrapper .commond .input-wrapper {
	width: 514rpx;
	height: 65rpx;
	background-color: rgb(244, 244, 244);
	position: relative;
}

.wrapper .commond .input-wrapper input {
	width: 100%;
	height: 100%;
	font-size: 24rpx;
	color: #666;
	box-sizing: border-box;
	padding: 0 20rpx;
	padding-left: 60rpx;
}

.wrapper .commond .input-wrapper image {
	width: 25rpx;
	height: 25rpx;
	position: absolute;
	left: 20rpx;
	top: 50%;
	transform: translateY(-50%);
}

.wrapper .commond .icon:nth-of-type(1) {
	width: 40rpx;
	height: 47rpx;
}

.wrapper .commond .icon:nth-of-type(2) {
	width: 52rpx;
	height: 38rpx;
}